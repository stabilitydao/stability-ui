---
import Layout from '@layouts/Layout.astro';

import {Metavault} from "@modules";

import { deployments } from '@stabilitydao/stability';

import type {TAddress} from '@types';

const { metavault } = Astro.params as {metavault:TAddress};
---

<Layout title={"Meta Vault - Stability"}
  img="/ogs/metavaults.png"
  metaTitle="On-chain asset management, liquidity mining and yield farming platform."
  metaUrl="https://stability.farm/metavaults"
  metaDescription="Metavaults are automated vaults that combine multiple DeFi protocols and assets into a single strategy while automatically rebalancing positions across integrated DeFi protocols and assets, maximizing returns"
>
        <Metavault metavault={metavault} client:only="react" />
</Layout>
